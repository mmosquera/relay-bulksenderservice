  <appSettings>
    <!--SMTP SETTINGS-->
    <add key="smtpHost" value="DopplerRelayQA.makingsense.com" />
    <add key="smtpPort" value="2525" />
    <!--API SETTINGS-->
    <add key="BaseUrl" value="http://dopplerrelayqa.makingsense.com:8080" />
    <add key="AccountUrl" value="/accounts/{AccountId}" />
    <add key="TemplateUrl" value="/accounts/{AccountId}/templates/{TemplateId}/message" />
    <!--<add key="TokenUrl" value="/tokens" />-->

    <!--SMTP SETTINGS PROD-->
    <!--<add key="smtpHost" value="smtp.dopplerrelay.com" />
    <add key="smtpPort" value="587" />-->
    <!--API SETTINGS PROD-->
    <!--<add key="BaseUrl" value="http://api.dopplerrelay.com/"/>
