    <!--GENERAL SETTINGS-->
    <add key="FtpListInterval" value="600000" />
    <add key="ReportsInterval" value="60000" />
    <add key="CleanInterval" value="600000" />
    <add key="LocalDownloadFolder" value="c:\temp\localfile" />
    <add key="DeliveryInterval" value="100" />
    <add key="MaxNumberOfThreads" value="2" />
    <add key="BulkEmailCount" value="100" />
    <add key="CleanDays" value="30" />
    <add key="CleanAttachmentsDays" value="5"/>
    <!--ADMIN USER-->
    <add key="AdminUser" value="username1@dopplerrelay.com" />
    <add key="AdminPass" value="password" />
    <add key="ClientSettingsProvider.ServiceUri" value="" />
  </appSettings>
  <connectionStrings>
    <add name="RelayConnectionString" connectionString="Server=DopplerRelayQA.makingsense.com;Database=Relay;User Id=Relay; Password=g65.uEY6;" />
  </connectionStrings>
  <log4net>
    <appender name="FileAppender" type="log4net.Appender.RollingFileAppender">
      <rollingStyle value="Composite" />
      <maxSizeRollBackups value="-1" />
      <maximumFileSize value="2048KB" />
      <datePattern value="'BulkSenderService.'yyyy-MM-dd'.log'" />
      <staticLogFileName value="false" />
      <file value="C:/temp/" />
      <appendToFile value="true" />
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger [%property{NDC}] - %message%newline" />
      </layout>
    </appender>
    <logger name="BulkSenderService">
      <level value="ALL" />
      <appender-ref ref="FileAppender" />
    </logger>
  </log4net>
